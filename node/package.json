{
  "name": "sample-express",
  "version": "0.0.1",
  "description": "Sample express app.",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/app.ts",
    "start": "ts-node src/app.ts",
    "build": "tsc",
    "check:types": "tsc --noEmit",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prisma:seed": "prisma db seed",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:dev": "run-s -l prisma:generate prisma:migrate prisma:seed",
    "prisma:studio": "prisma studio",
    "test": "NODE_ENV=test jest",
    "test:sh": "sh test/test.sh",
    "dev:noPrisma": "nodemon --watch 'src/**/*.ts' --exec ts-node src/_noPrismaProject/index.ts",
    "test:noPrisma": "sh src/_noPrismaProject/test.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "5.2.0",
    "body-parser": "^1.20.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "@types/passport": "^1.0.12",
    "@types/passport-http": "^0.3.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prisma": "5.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
